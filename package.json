{
  "name": "contribution-tracker",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "npm run generate:api:watch & vite",
    "build": "npm run generate:api && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "generate:api": "scripts/generate-api.sh",
    "generate:api:watch": "chokidar 'openapi.json' -c 'npm run generate:api' --initial --silent",
    "generate:graphql": "deno run -A npm:@graphql-codegen/cli/graphql-codegen-esm --config graphql_codegen.ts"
  },
  "dependencies": {
    "@deno/vite-plugin": "^1.0.5",
    "@octokit/core": "^7.0.6",
    "@octokit/graphql": "^9.0.3",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-query-persist-client": "^5.90.14",
    "@types/react": "^19.2.7",
    "@vitejs/plugin-react": "^5.1.2",
    "idb-keyval": "^6.2.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-error-boundary": "^6.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^6.0.0",
    "@graphql-codegen/cli": "^6.1.0",
    "@graphql-codegen/typescript": "^5.0.6",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.4",
    "@types/react-dom": "^19.2.3",
    "@vitest/ui": "^4.0.16",
    "chokidar-cli": "^3.0.0",
    "globals": "^16.5.0",
    "happy-dom": "^20.0.11",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  }
}
